<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- Mapper 인터페이스와 연결하는 방법 : namespace 속성 값으로 Mapper 인터페이스의 패키지명 + 인터페이명 작성 -->
<mapper namespace="edu.kh.repet.board.mapper.EditBoardMapper">
  
 
	<insert id="insertBoardImg" parameterType="BoardImg">
		INSERT INTO "BOARD_IMG"
		
		<foreach collection="list" item="img"
				  open="(" close=")" separator="UNION ALL">
			SELECT
				NEXT_IMG_NO(),
				#{img.imgPath},
				#{img.imgRename},
				#{img.boardNo}
			FROM DUAL
		</foreach>
	</insert>
	
	<insert id="insertBoard" parameterType="Board" useGeneratedKeys="true">
	
		<selectKey order="BEFORE" resultType="_int" keyProperty="boardNo">
			SELECT SEQ_BOARD_NO.NEXTVAL FROM DUAL
		</selectKey>
		INSERT INTO "BOARD"
		V
	</insert>
	

















</mapper>
